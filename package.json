    "web3-accounts": "1.2.3",
    "web3-wallets": "^2.1.1"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.16.7",
    "@ensdomains/ensjs": "^2.1.0",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^12.20.41",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "antd": "^4.19.3",
    "assert": "^2.0.0",
