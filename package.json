    "css-loader": "^6.7.1",
    "eslint": "^7.32.0",
    "html-webpack-plugin": "^5.5.0",
    "https-browserify": "^1.0.0",
    "os-browserify": "^0.3.0",
    "process": "^0.11.10",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "sass": "^1.49.10",
    "sass-loader": "^12.6.0",
    "sinon": "^12.0.1",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
